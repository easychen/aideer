{
  "name": "aideer",
  "productName": "AiDeer",
  "version": "1.0.3",
  "description": "AI assets manage tool",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "dev": "electron src/main.js",
    "build": "echo 'No build needed for JS files'",
    "start": "electron src/main.js",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on dist/main.js && electron dist/main.js\"",
    "build:all": "node build.cjs",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "package86": "electron-forge package --arch x64 --platform darwin",
    "packageuni": "electron-forge package --arch universal --platform darwin",
    "publish": "electron-forge publish",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [],
  "author": {
    "name": "Easy",
    "email": "easychen@qq.com"
  },
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "shell-env": "^4.0.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-dmg": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "eslint": "^8.56.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "wait-on": "^7.2.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "AiDeer",
        "executableName": "AiDeer",
        "icon": "icon.icns",
        "arch": "universal",
        "extraResource": [
          "resources"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
